<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="androidx.ads.identifier.provider.testapp">

    <application
        android:allowBackup="false"
        tools:ignore="GoogleAppIndexingWarning">

        <meta-data
            android:name="androidx.ads.identifier.provider.impl"
            android:value="androidx.ads.identifier.provider.testapp.AdvertisingIdProvider" />

        <activity
            android:name=".AdsIdentifierProviderActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
